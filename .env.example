## Example environment variables for agentic-browser
# Copy this file to `.env` and fill in real values before running.

# App environment
ENV=development
DEBUG=True

# Backend host/port
BACKEND_HOST=0.0.0.0
BACKEND_PORT=5454

# Google API Key (used by default LLM provider and youtube utils)
# Obtain from Google Cloud Console and enable the appropriate APIs.
GOOGLE_API_KEY=

# OpenAI API Key (if using OpenAI provider)
OPENAI_API_KEY=

# Anthropic API Key (if using Anthropic provider)
ANTHROPIC_API_KEY=

# Ollama base URL (if using Ollama local/remote server)
OLLAMA_BASE_URL=http://localhost:11434

# Deepseek / Openrouter override API keys (optional)
DEEPSEEK_API_KEY=
OPENROUTER_API_KEY=

# If using any provider with a custom base URL, set it here
BASE_URL=
